<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simpolab.client_manager.homepage.HomepageController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="15.0" HBox.hgrow="ALWAYS">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Active sessions">
               <font>
                  <Font name="Lucida Sans Typewriter Regular" size="12.0" />
               </font></Text>
            <ListView fx:id="lvSessions" prefHeight="282.0" prefWidth="279.0" />
            <Button fx:id="btnOpenSession" mnemonicParsing="false" text="Open" />
         </children>
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="270.0" spacing="10.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Session">
                     <font>
                        <Font name="Lucida Sans Typewriter Regular" size="12.0" />
                     </font>
                  </Text>
                  <Button fx:id="btnNewSession" mnemonicParsing="false" onAction="#onBtnNewSessionClicked" text="New" />
                  <Button fx:id="btnManageSession" mnemonicParsing="false" onAction="#onBtnManageSessionClicked" text="Manage" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Electors">
                     <font>
                        <Font name="Lucida Sans Typewriter Regular" size="12.0" />
                     </font></Text>
                  <Button fx:id="btnNewElector" mnemonicParsing="false" onAction="#onBtnNewElectorClicked" text="New" />
                  <Button fx:id="btnManageElectors" mnemonicParsing="false" onAction="#onBtnManageElectorsClicked" text="Manage" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Group">
                     <font>
                        <Font name="Lucida Sans Typewriter Regular" size="12.0" />
                     </font>
                  </Text>
                  <Button fx:id="btnNewGroup" mnemonicParsing="false" onAction="#onBtnNewGroupClicked" text="New" />
                  <Button fx:id="btnManageGroup" mnemonicParsing="false" onAction="#onBtnManageGroupClicked" text="Manage" />
               </children>
            </VBox>
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</HBox>
